generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id          String @id @default(uuid())
  login       String @unique
  version     Int @default(1)
  createdAt   Int
  updatedAt   Int
  password    String
}

model Artist {
  id          String @id @default(uuid())
  name        String
  grammy      Boolean? @default(false)
}
